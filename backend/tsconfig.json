{
  "compilerOptions": {
    "module": "ESNext", // Ensure it's ESNext or ES6
    "target": "ES2018",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"
      ]
    }
  },
  "exclude": ["node_modules"]
}
